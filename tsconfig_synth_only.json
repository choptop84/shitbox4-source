{
	"files": [ "synth/synth.ts" ],
	"compilerOptions": {
		"rootDir": "./",
		"outDir": "build",
		"target": "es2016",
		"moduleResolution": "node",
		"sourceMap": true,
		"removeComments": true,
		"noImplicitAny": true,
		"noImplicitThis": true,
		"strictNullChecks": true,
		"strictFunctionTypes": true,
		"strictBindCallApply": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"noUnusedLocals": true
	}
}
